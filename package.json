{
  "name": "boilerplate-next",
  "version": "0.0.0",
  "scripts": {
    "build": "next build",
    "build-stats": "cross-env ANALYZE=true next build",
    "start": "cross-env NODE_ENV=production next build && next start",
    "start:node": "cross-env NODE_ENV=production next build && node ./server.js",
    "dev": "next dev",
    "lint": "next lint",
    "export": "next export",
    "update": "run-s update:check update:all",
    "update:check": "npm-check-updates",
    "update:all": "npm-check-updates -u"
  },
  "devDependencies": {
    "@serverless-guru/prettier-plugin-import-order": "0.4.2",
    "@types/node": "20.0.0",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "8.39.0",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "npm-check-updates": "16.10.9",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "sass": "1.62.1",
    "stylelint": "15.6.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard-scss": "9.0.0",
    "stylelint-scss": "5.0.0",
    "typescript": "5.0.4"
  },
  "keywords": [
    "nextjs",
    "boilerplate",
    "website"
  ],
  "author": "callmeumm",
  "engineStrict": true,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0"
  },
  "packageManager": "yarn@3.5.0",
  "dependencies": {
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "next": "13.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
